{
  "name": "backend",
  "version": "1.0.0",
  "description": "CA Marketplace Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "seed": "ts-node prisma/seed-analytics.ts",
    "seed:analytics": "ts-node prisma/seed-analytics.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit --coverage",
    "test:integration": "jest --testPathPattern=tests/integration --coverage --runInBand",
    "test:security": "jest --testPathPattern=tests/security --coverage --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:perf": "k6 run tests/performance/load-test.js",
    "test:perf:stress": "k6 run tests/performance/stress-test.js",
    "test:perf:api": "k6 run tests/performance/api-performance.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "tsc --noEmit",
    "db:maintenance": "ts-node src/scripts/db-maintenance.ts",
    "db:analyze": "ts-node src/scripts/db-maintenance.ts analyze",
    "db:vacuum": "ts-node src/scripts/db-maintenance.ts vacuum",
    "db:reindex": "ts-node src/scripts/db-maintenance.ts reindex",
    "db:stats": "ts-node src/scripts/db-maintenance.ts stats"
  },
  "keywords": [
    "ca",
    "marketplace",
    "accounting"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@sendgrid/mail": "^8.1.6",
    "@types/handlebars": "^4.0.40",
    "@types/node-cron": "^3.0.11",
    "bcrypt": "^6.0.0",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "csv-writer": "^1.6.0",
    "dataloader": "^2.2.2",
    "diff": "5.2.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "file-type": "^16.5.4",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^3.0.0-rc.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "4.17.23",
    "morgan": "^1.10.0",
    "multer": "^2.0.0-rc.4",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.13",
    "prom-client": "^15.1.0",
    "puppeteer": "^24.35.0",
    "rate-limit-redis": "^4.2.0",
    "razorpay": "^2.9.4",
    "socket.io": "^4.8.1",
    "typescript": "^5.7.3",
    "uuid": "^9.0.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^6.4.14",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "^6.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
